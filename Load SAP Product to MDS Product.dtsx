    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{C00AC7FA-19EA-40CA-B129-FEA878EE3214}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT mm.[MATNR] AS [Product Code]&#xA;, CAST (COALESCE(mt.TEXT, md.MAKTX) as nvarchar(50) ) AS [Product Name]&#xA;, LEFT(mm.MATNR, 5) AS [Product Base Code]&#xA;, md.[MAKTX] AS [Product Base Name]&#xA;, mm.[MSTAV] AS [Product Status]&#xA;, lp.[KBETR] AS [Product Current List Price]&#xA;, mm.[MHDHB] AS [Product Total Shelf Life]&#xA;, um.[MEINH] AS [Product Packaging UOM]&#xA;, mp.[WZEIT] AS [Product Lead Time]&#xA;&#xA;&#xA;&#xA;from [SAPStaging].[dbo].[tmp_material_master_CT] mm&#xA;join [SAPStaging].[dbo].[MATERIAL_DESCS] md on md.[MATNR] = mm.MATNR&#xA;left join [SAPStaging].[dbo].[MATERIAL_PLANTS] mp on mp.[MATNR] = mm.MATNR&#xA;left join [SAPStaging].[dbo].[MATERIAL_TEXTS] mt on mt.[MATNR] = mm.MATNR and mt.VKORG = '9999' and mt.VTWEG = '99'&#xA;--left join [SAPStaging].[dbo].[MATERIAL_TEXTS] mt2 on mt2.[MATNR] = left(mm.MATNR, 5) and mt2.VKORG = '9999' and mt2.VTWEG = '99'&#xA;left join  [SAPStaging].[dbo].[MATERIAL_ALT_UOM]  um on um.MATNR = mm.MATNR and um.MEINH = mm.MEINS&#xA;left join (&#x9;select MATNR, KBETR, DATAB, DATBI  &#xA;&#x9;&#x9;&#x9;from [SAPStaging].[dbo].[MATERIAL_LIST_PRICE]&#xA;&#x9;&#x9;&#x9;where VKORG  = 1000 -- SalesOrganization&#xA;&#x9;&#x9;&#x9;and VTWEG = 10 -- DistributionChannel&#xA;&#x9;&#x9;&#x9;and isnull(LOEVM_KO,'') &lt;&gt; 'X' -- product hasn't been deleted&#xA;&#x9;&#x9;&#x9;and convert(date, getdate(),126) between DATAB and DATBI) lp on lp.[MATNR] = mm.[MATNR]&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL">
      <DTS:VariableValue
        DTS:DataType="8">SELECT mm.[MATNR] AS [Product Code]
, CAST (COALESCE(mt.TEXT, md.MAKTX) as nvarchar(50) ) AS [Product Name]
, LEFT(mm.MATNR, 5) AS [Product Base Code]
, md.[MAKTX] AS [Product Base Name]
, mm.[MSTAV] AS [Product Status]
, lp.[KBETR] AS [Product Current List Price]
, mm.[MHDHB] AS [Product Total Shelf Life]
, um.[MEINH] AS [Product Packaging UOM]
, mp.[WZEIT] AS [Product Lead Time]



from [SAPStaging].[dbo].[tmp_material_master_CT] mm
join [SAPStaging].[dbo].[MATERIAL_DESCS] md on md.[MATNR] = mm.MATNR
left join [SAPStaging].[dbo].[MATERIAL_PLANTS] mp on mp.[MATNR] = mm.MATNR
left join [SAPStaging].[dbo].[MATERIAL_TEXTS] mt on mt.[MATNR] = mm.MATNR and mt.VKORG = '9999' and mt.VTWEG = '99'
--left join [SAPStaging].[dbo].[MATERIAL_TEXTS] mt2 on mt2.[MATNR] = left(mm.MATNR, 5) and mt2.VKORG = '9999' and mt2.VTWEG = '99'
left join  [SAPStaging].[dbo].[MATERIAL_ALT_UOM]  um on um.MATNR = mm.MATNR and um.MEINH = mm.MEINS
left join (	select MATNR, KBETR, DATAB, DATBI  
			from [SAPStaging].[dbo].[MATERIAL_LIST_PRICE]
			where VKORG  = 1000 -- SalesOrganization
			and VTWEG = 10 -- DistributionChannel
			and isnull(LOEVM_KO,'') &lt;&gt; 'X' -- product hasn't been deleted
			and convert(date, getdate(),126) between DATAB and DATBI) lp on lp.[MATNR] = mm.[MATNR]</DTS:VariableValue>
    </DTS:Variable>
