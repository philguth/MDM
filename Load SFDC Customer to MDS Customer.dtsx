      DTS:CreationName=""
      DTS:DTSID="{BA72A84D-2E68-4788-8A9C-014CDE9575FE}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT SFDC_18_CHAR_ID__c + '_' + ISNULL(Parentid, SFDC_18_CHAR_ID__c) AS [Customer Bill To Ship To]&#xA;, a.[Name] AS [Customer Bill To Name]&#xA;, a.[BillToAddress1__c] AS [Customer Bill To Address 1]&#xA;, a.[BillToAddress2__c] AS [Customer Bill To Address 2]&#xA;, a.[BillToAddress3__c] AS [Customer Bill To Address 3]&#xA;, a.[BillingCity] AS [Customer Bill To City]&#xA;, a.[BillingState] AS [Customer Bill To State]&#xA;, a.[BillingPostalCode] AS [Customer Bill To Postal Code]&#xA;, CASE WHEN [BillingCountry] = 'UK' Then 'GB' ELSE [BillingCountry] END AS [Customer Bill To Country]&#xA;, a.[BillToParentCustomerCode__c] AS [Customer Functional Parent ID]&#xA;, a.[ShipToAddressName__c] AS [Customer Ship To Name]&#xA;, a.[ShipToAddress1__c] AS [Customer Ship To Address 1]&#xA;, a.[ShipToAddress2__c] AS [Customer Ship To Address 2]&#xA;, a.[ShippingCity] AS [Customer Ship To City]&#xA;, a.[ShippingState] AS [Customer Ship To State]&#xA;, a.[ShippingPostalCode] AS [Customer Ship To Postal Code]&#xA;, CASE WHEN ShippingCountry = 'UK' Then 'GB' ELSE ShippingCountry END AS [Customer Ship To Country]&#xA;, a.[ShippingCountry] AS [Customer Destination Country]&#xA;, a.[ShipToParentcustomerCode__c] AS [Customer Logical Parent ID]&#xA;, CASE WHEN Account_Status__c = 'Active' Then 1 WHEN Account_Status__c = 'Inactive' THEN 0 ELSE Account_Status__c END AS [Customer Active Account]&#xA;, CASE WHEN IsMainAccount__c = 'True' Then 1 WHEN IsMainAccount__c = 'False' THEN 0 ELSE IsMainAccount__c END AS [Customer Primary Account]&#xA;, CASE WHEN US_FreeShipping__c = 'True' Then 1 WHEN US_FreeShipping__c = 'False' THEN 0 ELSE US_FreeShipping__c END AS [Customer Free Shipping]&#xA;, CASE WHEN US_FreezerOrDirect__c = 'Direct' THEN 0 WHEN US_FreezerOrDirect__c like 'freezer%' THEN 1 ELSE 0 END AS [Customer Freezer]&#xA;, isnull([discount_rate__c], '.00') AS [Customer Discount]&#xA;, a.[US_CustomerGroup__c] AS [Customer Industry Code]&#xA;, a.[Type] AS [Customer Account Type]&#xA;, a.[US_CustomerGroup__c] AS [Customer Group]&#xA;, a.[Rating] AS [Customer Rating]&#xA;, CAST([Account_SFDC_ID__c] as nvarchar(15)) AS [Customer Salesforce ID]&#xA;, a.[CreatedDate] AS [Customer Salesforce Create Date]&#xA;, N'Salesforce' AS [Customer Source]&#xA;, a.[rollup_First_Date_of_Sale__c] AS [Customer First Sale Date]&#xA;, a.[NEB_Division__c] AS [Customer NEB Division]&#xA; FROM [salesforce backups].dbo.Account a INNER JOIN&#xA; [dbo].[RecordType] ON (RecordType.id = a.recordtypeid)&#xA;  Where (Account_Originating_Source__c &lt;&gt; 'SAP'&#xA; or Account_Originating_Source__c is null)&#xA; and [dbo].[RecordType].name &lt;&gt; 'Inactive Account'&#xA; and Account_Status__c &lt;&gt; 'Inactive'&#xA; and [dbo].[RecordType].name = 'UK Standard Account'&#xA;AND ([Account_SFDC_ID__c] like '0013000001E9de0' or [ParentId] like '0013000001E9de0%')&#xA; order by SFDC_18_CHAR_ID__c&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL">
      <DTS:VariableValue
        DTS:DataType="8">SELECT SFDC_18_CHAR_ID__c + '_' + ISNULL(Parentid, SFDC_18_CHAR_ID__c) AS [Customer Bill To Ship To]
, a.[Name] AS [Customer Bill To Name]
, a.[BillToAddress1__c] AS [Customer Bill To Address 1]
, a.[BillToAddress2__c] AS [Customer Bill To Address 2]
, a.[BillToAddress3__c] AS [Customer Bill To Address 3]
, a.[BillingCity] AS [Customer Bill To City]
, a.[BillingState] AS [Customer Bill To State]
, a.[BillingPostalCode] AS [Customer Bill To Postal Code]
, CASE WHEN [BillingCountry] = 'UK' Then 'GB' ELSE [BillingCountry] END AS [Customer Bill To Country]
, a.[BillToParentCustomerCode__c] AS [Customer Functional Parent ID]
, a.[ShipToAddressName__c] AS [Customer Ship To Name]
, a.[ShipToAddress1__c] AS [Customer Ship To Address 1]
, a.[ShipToAddress2__c] AS [Customer Ship To Address 2]
, a.[ShippingCity] AS [Customer Ship To City]
, a.[ShippingState] AS [Customer Ship To State]
, a.[ShippingPostalCode] AS [Customer Ship To Postal Code]
, CASE WHEN ShippingCountry = 'UK' Then 'GB' ELSE ShippingCountry END AS [Customer Ship To Country]
, a.[ShippingCountry] AS [Customer Destination Country]
, a.[ShipToParentcustomerCode__c] AS [Customer Logical Parent ID]
, CASE WHEN Account_Status__c = 'Active' Then 1 WHEN Account_Status__c = 'Inactive' THEN 0 ELSE Account_Status__c END AS [Customer Active Account]
, CASE WHEN IsMainAccount__c = 'True' Then 1 WHEN IsMainAccount__c = 'False' THEN 0 ELSE IsMainAccount__c END AS [Customer Primary Account]
, CASE WHEN US_FreeShipping__c = 'True' Then 1 WHEN US_FreeShipping__c = 'False' THEN 0 ELSE US_FreeShipping__c END AS [Customer Free Shipping]
, CASE WHEN US_FreezerOrDirect__c = 'Direct' THEN 0 WHEN US_FreezerOrDirect__c like 'freezer%' THEN 1 ELSE 0 END AS [Customer Freezer]
, isnull([discount_rate__c], '.00') AS [Customer Discount]
, a.[US_CustomerGroup__c] AS [Customer Industry Code]
, a.[Type] AS [Customer Account Type]
, a.[US_CustomerGroup__c] AS [Customer Group]
, a.[Rating] AS [Customer Rating]
, CAST([Account_SFDC_ID__c] as nvarchar(15)) AS [Customer Salesforce ID]
, a.[CreatedDate] AS [Customer Salesforce Create Date]
, N'Salesforce' AS [Customer Source]
, a.[rollup_First_Date_of_Sale__c] AS [Customer First Sale Date]
, a.[NEB_Division__c] AS [Customer NEB Division]
 FROM [salesforce backups].dbo.Account a INNER JOIN
 [dbo].[RecordType] ON (RecordType.id = a.recordtypeid)
  Where (Account_Originating_Source__c &lt;&gt; 'SAP'
 or Account_Originating_Source__c is null)
 and [dbo].[RecordType].name &lt;&gt; 'Inactive Account'
 and Account_Status__c &lt;&gt; 'Inactive'
 and [dbo].[RecordType].name = 'UK Standard Account'
AND ([Account_SFDC_ID__c] like '0013000001E9de0' or [ParentId] like '0013000001E9de0%')
 order by SFDC_18_CHAR_ID__c</DTS:VariableValue>
    </DTS:Variable>
