    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{047B9CED-54E4-4C89-A93A-333316C24805}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT ii.VBELN + '_' + CAST(ii.POSNR AS nvarchar(10)) AS [Invoice Line Number]&#xA;, ih.[VBELN] AS [Invoice Header Invoice Number]&#xA;, ih.[FKDAT] AS [Invoice Header Date]&#xA;, ih.[KUNAG] + '_' + dp.[KUNNR] AS [Bill To Ship To Code]&#xA;, ih.[BSTNK_VF] AS [Invoice Header Customer Purchase Order Number]&#xA;, CAST(dt_po_date.DATA AS NVARCHAR(25)) AS [Invoice Item Purchase Order Date]&#xA;, oh.[BSARK] AS [Order Header Purchase Order Type]&#xA;, ih.[FKART] AS [Invoice Header Billing Type]&#xA;, ih.[FKTYP] AS [Invoice Header Billing Category]&#xA;, ih.[WAERK] AS [Invoice Header Local Currency Code]&#xA;, CASE WHEN  ih.VBTYP in ('O', 'N') THEN  -1 * (ih.[NETWR] - ISNULL(freight.[NETWR],0)) else ISNULL(ih.[NETWR],0) - ISNULL(freight.[NETWR],0) END AS [Invoice Header Net Value in Local Currency]&#xA;, CASE WHEN ih.VBTYP in ('O', 'N') THEN -1 *ISNULL(freight.[NETWR],0) ELSE ISNULL(freight.[NETWR],0) END AS [Invoice Header Freight Cost]&#xA;, ii.[VGBEL] AS [Invoice Item Delivery Number]&#xA;, dh.[LFDAT] AS [Delivery Header Delivery Date]&#xA;, ii.[MATNR] AS [Invoice Item Product Code]&#xA;, ii.[ARKTX] AS [Invoice Item Description]&#xA;, ii.[MATKL] AS [Invoice Item Product Type]&#xA;, ii.[PSTYV] AS [Invoice Item Category]&#xA;, ISNULL(ii.FKIMG,0) AS [Invoice Item Actual Invoiced Quantity]&#xA;, ISNULL(ii.KZWI2, 0) AS [Invoice Item Discount]&#xA;, ii.[PRCTR] AS [Invoice Item Profit Center]&#xA;, ii.[AUBEL] AS [Invoice Item Order Number]&#xA;, ii.[AUPOS] AS [Order Item Number]&#xA;, CASE WHEN  ih.VBTYP in ('O', 'N') THEN -1 * ISNULL(ii.NETWR,0) ELSE ISNULL(ii.NETWR,0) END AS [Invoice Item Net Value in Local Currency]&#xA;, ISNULL(ii.MWSBP,0) AS [Invoice Item Tax Amount in Local Currency]&#xA;, CAST(dt_reference_ID.DATA AS NVARCHAR(100)) AS [Invoice Item Reference ID]&#xA;, CAST(dt_sender_ID.DATA AS NVARCHAR(20)) AS [Invoice Item XML Sender ID]&#xA;, CAST(dt_cXML_from_ID.DATA AS NVARCHAR(20)) AS [Invoice Item XML From ID]&#xA;, CAST(dt_ext_ship_to.DATA AS NVARCHAR(20)) AS [Invoice Item External Ship To]&#xA;, CAST(dt_ext_bill_to.DATA AS NVARCHAR(20)) AS [Invoice Item External Bill To]&#xA;, oh.[VTWEG] AS [Order Header Distribution Channel]&#xA;, oh.[SPART] AS [Order Header Division]&#xA;, oh.[VKORG] AS [Order Header Sales Organization]&#xA;, CASE WHEN FKSTO = 'X' Then 1 ELSE 0 END AS [Invoice Header Cancelled Invoice Flag]&#xA;, ih.[AEDAT] AS [Invoice Header Cancelled Invoice Date]&#xA;, ih.[SFAKN] AS [Invoice Header Associated Invoice Number]&#xA;, N'SAPStaging' AS [Database Source] &#xA;&#xA;FROM tmp_Invoice_Header_CT ih &#xA;--FROM [SAPStaging].dbo.INVOICE_HEADER ih&#xA;       INNER JOIN [SAPStaging].dbo.INVOICE_ITEMS ii ON (ih.vbeln = ii.vbeln and ii.PRCTR is not null )--and ii.netwr &lt;&gt; 0 )&#xA;&#x9;   INNER JOIN [SAPStaging].dbo.DOCUMENT_PARTNERS dp ON (ih.vbeln = dp.vbeln and dp.parvw = 'WE') --Ship To Code&#xA;       LEFT JOIN [SAPStaging].dbo.ORDER_HEADER oh ON (ii.aubel = oh.vbeln)&#xA;&#x9;   LEFT JOIN [SAPStaging].dbo.DELIVERY_HEADER dh ON (ii.VGBEL = dh.vbeln)&#xA;&#x9;   LEFT JOIN [SAPStaging].dbo.INVOICE_ITEMS freight ON (freight.vbeln = ii.vbeln and freight.vgbel = dh.vbeln and freight.matnr = 'FREIGHT' and freight.aubel is not null) &#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_reference_id ON (ii.aubel = dt_reference_id.VBELN and dt_reference_id.[TDID] = 'S015')&#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_sender_ID ON (ii.aubel = dt_sender_ID.VBELN and dt_sender_ID.[TDID] = 'S017')&#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_cXML_from_ID ON (ii.aubel = dt_cXML_from_ID.VBELN and dt_cXML_from_ID.[TDID] = 'S016')&#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_ext_ship_to ON (ii.aubel = dt_ext_ship_to.VBELN and dt_ext_ship_to.[TDID] = 'S018')&#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_ext_bill_to ON (ii.aubel = dt_ext_bill_to.VBELN and dt_ext_bill_to.[TDID] = 'S023')&#xA;       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_po_date ON (ii.aubel = dt_po_date.VBELN and dt_po_date.[TDID] = 'S026')&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL">
      <DTS:VariableValue
        DTS:DataType="8">SELECT ii.VBELN + '_' + CAST(ii.POSNR AS nvarchar(10)) AS [Invoice Line Number]
, ih.[VBELN] AS [Invoice Header Invoice Number]
, ih.[FKDAT] AS [Invoice Header Date]
, ih.[KUNAG] + '_' + dp.[KUNNR] AS [Bill To Ship To Code]
, ih.[BSTNK_VF] AS [Invoice Header Customer Purchase Order Number]
, CAST(dt_po_date.DATA AS NVARCHAR(25)) AS [Invoice Item Purchase Order Date]
, oh.[BSARK] AS [Order Header Purchase Order Type]
, ih.[FKART] AS [Invoice Header Billing Type]
, ih.[FKTYP] AS [Invoice Header Billing Category]
, ih.[WAERK] AS [Invoice Header Local Currency Code]
, CASE WHEN  ih.VBTYP in ('O', 'N') THEN  -1 * (ih.[NETWR] - ISNULL(freight.[NETWR],0)) else ISNULL(ih.[NETWR],0) - ISNULL(freight.[NETWR],0) END AS [Invoice Header Net Value in Local Currency]
, CASE WHEN ih.VBTYP in ('O', 'N') THEN -1 *ISNULL(freight.[NETWR],0) ELSE ISNULL(freight.[NETWR],0) END AS [Invoice Header Freight Cost]
, ii.[VGBEL] AS [Invoice Item Delivery Number]
, dh.[LFDAT] AS [Delivery Header Delivery Date]
, ii.[MATNR] AS [Invoice Item Product Code]
, ii.[ARKTX] AS [Invoice Item Description]
, ii.[MATKL] AS [Invoice Item Product Type]
, ii.[PSTYV] AS [Invoice Item Category]
, ISNULL(ii.FKIMG,0) AS [Invoice Item Actual Invoiced Quantity]
, ISNULL(ii.KZWI2, 0) AS [Invoice Item Discount]
, ii.[PRCTR] AS [Invoice Item Profit Center]
, ii.[AUBEL] AS [Invoice Item Order Number]
, ii.[AUPOS] AS [Order Item Number]
, CASE WHEN  ih.VBTYP in ('O', 'N') THEN -1 * ISNULL(ii.NETWR,0) ELSE ISNULL(ii.NETWR,0) END AS [Invoice Item Net Value in Local Currency]
, ISNULL(ii.MWSBP,0) AS [Invoice Item Tax Amount in Local Currency]
, CAST(dt_reference_ID.DATA AS NVARCHAR(100)) AS [Invoice Item Reference ID]
, CAST(dt_sender_ID.DATA AS NVARCHAR(20)) AS [Invoice Item XML Sender ID]
, CAST(dt_cXML_from_ID.DATA AS NVARCHAR(20)) AS [Invoice Item XML From ID]
, CAST(dt_ext_ship_to.DATA AS NVARCHAR(20)) AS [Invoice Item External Ship To]
, CAST(dt_ext_bill_to.DATA AS NVARCHAR(20)) AS [Invoice Item External Bill To]
, oh.[VTWEG] AS [Order Header Distribution Channel]
, oh.[SPART] AS [Order Header Division]
, oh.[VKORG] AS [Order Header Sales Organization]
, CASE WHEN FKSTO = 'X' Then 1 ELSE 0 END AS [Invoice Header Cancelled Invoice Flag]
, ih.[AEDAT] AS [Invoice Header Cancelled Invoice Date]
, ih.[SFAKN] AS [Invoice Header Associated Invoice Number]
, N'SAPStaging' AS [Database Source] 

FROM tmp_Invoice_Header_CT ih 
--FROM [SAPStaging].dbo.INVOICE_HEADER ih
       INNER JOIN [SAPStaging].dbo.INVOICE_ITEMS ii ON (ih.vbeln = ii.vbeln and ii.PRCTR is not null )--and ii.netwr &lt;&gt; 0 )
	   INNER JOIN [SAPStaging].dbo.DOCUMENT_PARTNERS dp ON (ih.vbeln = dp.vbeln and dp.parvw = 'WE') --Ship To Code
       LEFT JOIN [SAPStaging].dbo.ORDER_HEADER oh ON (ii.aubel = oh.vbeln)
	   LEFT JOIN [SAPStaging].dbo.DELIVERY_HEADER dh ON (ii.VGBEL = dh.vbeln)
	   LEFT JOIN [SAPStaging].dbo.INVOICE_ITEMS freight ON (freight.vbeln = ii.vbeln and freight.vgbel = dh.vbeln and freight.matnr = 'FREIGHT' and freight.aubel is not null) 
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_reference_id ON (ii.aubel = dt_reference_id.VBELN and dt_reference_id.[TDID] = 'S015')
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_sender_ID ON (ii.aubel = dt_sender_ID.VBELN and dt_sender_ID.[TDID] = 'S017')
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_cXML_from_ID ON (ii.aubel = dt_cXML_from_ID.VBELN and dt_cXML_from_ID.[TDID] = 'S016')
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_ext_ship_to ON (ii.aubel = dt_ext_ship_to.VBELN and dt_ext_ship_to.[TDID] = 'S018')
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_ext_bill_to ON (ii.aubel = dt_ext_bill_to.VBELN and dt_ext_bill_to.[TDID] = 'S023')
       LEFT JOIN [SAPStaging].[dbo].[DOCUMENT_TEXTS] dt_po_date ON (ii.aubel = dt_po_date.VBELN and dt_po_date.[TDID] = 'S026')</DTS:VariableValue>
    </DTS:Variable>
