    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{39B2CE1D-1D48-42E7-9277-515FD43F03AA}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT distinct cp_bill_to.kunn2 + '_' +  ISNULL(cp_ship_to.kunn2, '0000000000') AS [Bill To Ship To]&#xA;, cp_bill_to.[KUNN2] AS [Bill To SAP ID]&#xA;, ca_bill_to.[SORT1] AS [Bill To Epicor ID]&#xA;, ca_bill_to.[NAME1] AS [Bill To Name]&#xA;, ca_bill_to.[STREET] AS [Bill To Address 1]&#xA;, ca_bill_to.[CITY1] AS [Bill To City]&#xA;, ca_bill_to.[REGION] AS [Bill To State]&#xA;, ca_bill_to.[POST_CODE1] AS [Bill To Postal Code]&#xA;, ca_bill_to.[COUNTRY] AS [Bill To Country]&#xA;, ca_ship_to.[STREET] AS [Ship To Address 1]&#xA;, ISNULL(cp_ship_to.kunn2, '0000000000') AS [Ship To SAP ID]&#xA;, ca_ship_to.[SORT1] AS [Ship To Epicor ID]&#xA;, ca_ship_to.[NAME1] AS [Ship To Name]&#xA;, ca_ship_to.[CITY1] AS [Ship To City]&#xA;, ca_ship_to.[REGION] AS [Ship To State]&#xA;, ca_ship_to.[POST_CODE1] AS [Ship To Postal Code]&#xA;, ca_ship_to.[COUNTRY] AS [Ship To Country]&#xA;, N'SAPStaging' AS [Customer Source]&#xA;--FROM CHANGETABLE(CHANGES [SAPStaging].[dbo].[CUSTOMER_MASTER],0)  cm&#xA;FROM tmp_Customer_Master_CT cm&#xA; INNER JOIN CUSTOMER_PARTNERS cp_bill_to ON (cm.kunnr = cp_bill_to.kunnr and cp_bill_to.PARVW = 'RE')&#xA;&#x9;INNER JOIN CUSTOMER_PARTNERS cp_ship_to ON (cp_bill_to.kunnr = cp_ship_to.kunnr and cp_ship_to.PARVW = 'WE')&#xA;&#x9;&#x9;LEFT JOIN CUSTOMER_ADDRESS ca_ship_to ON (cp_ship_to.kunn2 = ca_ship_to.kunnr)&#xA;&#x9;&#x9;LEFT JOIN CUSTOMER_ADDRESS ca_bill_to ON  (cp_bill_to.kunn2 = ca_bill_to.kunnr)&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL">
      <DTS:VariableValue
        DTS:DataType="8">SELECT distinct cp_bill_to.kunn2 + '_' +  ISNULL(cp_ship_to.kunn2, '0000000000') AS [Bill To Ship To]
, cp_bill_to.[KUNN2] AS [Bill To SAP ID]
, ca_bill_to.[SORT1] AS [Bill To Epicor ID]
, ca_bill_to.[NAME1] AS [Bill To Name]
, ca_bill_to.[STREET] AS [Bill To Address 1]
, ca_bill_to.[CITY1] AS [Bill To City]
, ca_bill_to.[REGION] AS [Bill To State]
, ca_bill_to.[POST_CODE1] AS [Bill To Postal Code]
, ca_bill_to.[COUNTRY] AS [Bill To Country]
, ca_ship_to.[STREET] AS [Ship To Address 1]
, ISNULL(cp_ship_to.kunn2, '0000000000') AS [Ship To SAP ID]
, ca_ship_to.[SORT1] AS [Ship To Epicor ID]
, ca_ship_to.[NAME1] AS [Ship To Name]
, ca_ship_to.[CITY1] AS [Ship To City]
, ca_ship_to.[REGION] AS [Ship To State]
, ca_ship_to.[POST_CODE1] AS [Ship To Postal Code]
, ca_ship_to.[COUNTRY] AS [Ship To Country]
, N'SAPStaging' AS [Customer Source]
--FROM CHANGETABLE(CHANGES [SAPStaging].[dbo].[CUSTOMER_MASTER],0)  cm
FROM tmp_Customer_Master_CT cm
 INNER JOIN CUSTOMER_PARTNERS cp_bill_to ON (cm.kunnr = cp_bill_to.kunnr and cp_bill_to.PARVW = 'RE')
	INNER JOIN CUSTOMER_PARTNERS cp_ship_to ON (cp_bill_to.kunnr = cp_ship_to.kunnr and cp_ship_to.PARVW = 'WE')
		LEFT JOIN CUSTOMER_ADDRESS ca_ship_to ON (cp_ship_to.kunn2 = ca_ship_to.kunnr)
		LEFT JOIN CUSTOMER_ADDRESS ca_bill_to ON  (cp_bill_to.kunn2 = ca_bill_to.kunnr)</DTS:VariableValue>
    </DTS:Variable>
